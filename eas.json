{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "environment": "preview",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "channel": "production"
    },
    "internal": {
      "environment": "production",
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "internal": {
      "android": {
        "track": "internal"
      }
    }
  }
}